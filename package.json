{
  "devDependencies": {
    "autoprefixer": "^10.2.6",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.2.4"
  },
  "scripts": {
    "build:css": "postcss ./public/css/style.css -o ./public/css/tailwind.css",
    "build:a": "NODE_ENV=production postcss ./src/tailwind.css -o ./public/css/tailwind.css",
    "build:tailwind": "postcss ./public/css/style.css -o ./public/css/prueba.css",
    "build:css2": "npx tailwindcss-cli@latest build -i ./public/css/style.css -o ./public/css/prueba.css"
  },
  "dependencies": {
    "win-node-env": "^0.4.0"
  }
}